Scan all the elements from right to left in array and keep track of maximum till now.
When maximum changes itâ€™s value, print it.


////////////////////////////////////////////////////////////////////////////////////////////
#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
	//code
	int t;
	long int n,i,j,max,count=0;
	cin>>t;
	while(t--)
	{
	    cin>>n;
	    count=0;
	    long int a[n],temp[n];
	    memset(a,0,sizeof(a));
	    for(i=0;i<n;i++)
	    {
	        cin>>a[i];
	    }  
	    max=a[n-1];
	    for(i=n-1;i>=0;i--)
	    {
	        if(a[i]>=max)
	         {
	          max=a[i];
	          temp[count]=a[i];
	          count++;
	         }
	    }
	   for(i=count-1;i>=0;i--)
	    {
	        cout<<temp[i]<<" ";
	    }
	    cout<<endl;
	 
	   
	}
	return 0;
}
