#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t; //Input testcases
    while(t--)
    {
        int j,n,i,a,fact=0;
       cin>>n; //Input size of array
       int gcd=0; 
       for(i=0;i<n;i++)
       {
           cin>>a; //input elements of array
           gcd=__gcd(gcd,a); //overall gcd is gcd(gcd, element)
       }
       cout<<gcd<<endl;
    }
    return 0;
}
